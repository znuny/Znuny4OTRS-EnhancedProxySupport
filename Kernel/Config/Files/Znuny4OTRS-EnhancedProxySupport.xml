<?xml version="1.0" encoding="utf-8" ?>
<otrs_config version="1.0" init="Application">
    <ConfigItem Name="WebUserAgent::NoProxy" Required="0" Valid="0" ConfigLevel="200">
        <Description Translatable="1">Defines the URLs/Hosts for http/ftp, where no proxy is used.</Description>
        <Group>Framework</Group>
        <SubGroup>Core::WebUserAgent</SubGroup>
        <Setting>
            <String Regex="">host1.example.com;host2.example.com</String>
        </Setting>
    </ConfigItem>
    <ConfigItem Name="WebUserAgent::UserAgent" Required="0" Valid="0" ConfigLevel="200">
        <Description Translatable="1">Configure the content of the header UserAgent.</Description>
        <Group>Framework</Group>
        <SubGroup>Core::WebUserAgent</SubGroup>
        <Setting>
            <String Regex=""></String>
        </Setting>
    </ConfigItem>
</otrs_config>
